<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >

<suite name="Suite1" verbose="1">

    <test name="Exclude Broken Tests">
        <groups>
            <run>
                <exclude name="broken"  />
                <include name="working"  />
            </run>
        </groups>

        <packages>
            <package name="Tests.PackageOfTests"/>
        </packages>
    </test>



</suite>
